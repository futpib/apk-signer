{
  "name": "@futpib/apk-signer",
  "version": "1.0.0",
  "main": "build/index.js",
  "license": "GPL-3.0-only",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "xo && c8 ava",
