  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "@types/invariant": "^2.2.37",
    "@types/node": "^22.10.2",
    "@types/node-forge": "^1.3.11",
    "ava": "^6.2.0",
    "c8": "^10.1.3",
    "coveralls": "^3.1.1",
    "eslint-config-xo-overrides": "^1.6.0",
    "typescript": "^5.7.2",
    "xo": "^0.60.0"
  },
  "xo": {
    "extends": [
      "eslint-config-xo-overrides"
    ],
    "rules": {
      "ava/no-ignored-test-files": "off"
    }
  },
  "packageManager": "yarn@4.5.3",
  "dependencies": {
    "@futpib/parser": "^1.0.1",
    "invariant": "^2.2.4",
    "node-forge": "^1.3.1"
  }
